for file in $files
do
	bowtie2 --sensitive -x ~/sf_ubuntushare/scovelli_genome/ssc_chromonome \
		-S ${file}.sam -t \
		-U ${file}.fq \
		--un ${file}.unpaired.sam \
		-p 3

done 2>&1 | tee  bowtie.sensitive.fwsw.log

